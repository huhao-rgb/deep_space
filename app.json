{
  "expo": {
    "name": "深空场",
    "slug": "deep-space",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.miniflower.deepspace"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "ce54d4c0-d125-4cd8-b72f-5674671776d0"
      }
    },
    "owner": "miniflower",
    "plugins": [
      "expo-router"
    ],
    "scheme": "deep-space-scheme"
  }
}
